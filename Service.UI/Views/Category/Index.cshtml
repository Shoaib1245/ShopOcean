@model Service.UI.Models.CategoryModel
@{

    Layout = "~/Views/Shared/_SellerLayout.cshtml";
}

<div class="layout-px-spacing">

    <div class="middle-content container-xxl p-0">

        <!-- BREADCRUMB -->
        <div class="page-meta">
            <nav class="breadcrumb-style-one" aria-label="breadcrumb">
                <ol class="breadcrumb">

                </ol>
            </nav>
        </div>
        <!-- /BREADCRUMB -->



        <div class="seperator-header">
            <button onclick="ModelShows();" class="btn btn-outline-primary">Add Category</button>
      </div>

        <div class="row layout-spacing">
            <div class="col-lg-12">
                <div class="statbox widget box box-shadow">
                    <div class="widget-content widget-content-area">
                        
                        <div id="TableShow">

                        </div>


                    </div>
                </div>
            </div>
        </div>

    </div>

</div>

<div id="ModelZoom" class="modal animated zoomInUp custo-zoomInUp">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div id="ModelShow">
        </div>
    </div>
</div>



<script>
    $(document).ready(function(){
        TableShows();
    })

    function ModelShows(){
        debugger;
        $.ajax({
            type:'get',
            url: '/Category/ModelPopShow',
            success:function(res){
              
                $('#ModelShow').html(res);
                $('#ModelZoom').modal('show');
            }
        })
    }


    function TableShows() {
        debugger;
        $.ajax({
            type: 'get',
            url: '/Category/TablePartial',
            success: function (res) {

                $('#TableShow').html(res);
            }
        })
    }

</script>