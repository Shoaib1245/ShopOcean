<div class="hot-deal-products off-white-bg pb-90 pb-sm-50">
    <div class="container">
        <!-- Product Title Start -->
        <div class="post-title pb-30">
            <h2>hot deals</h2>
        </div>
        <!-- Product Title End -->
        <!-- Hot Deal Product Activation Start -->
        <div class="hot-deal-active owl-carousel">
            @foreach (var item in Model.ProductData.Where(p => p.Discount >= 10).OrderByDescending(a => a.Discount).Take(10))
            {
                <div class="single-product">
                    <!-- Product Image Start -->
                    <div class="pro-img">
                        <a href="product.html">
                            <img class="imagee" src="@item.Image.Split(',')[0]" alt="single-product">
                            <img class="secondary-img" style="height:221px;  width: 221px;" !important src="@item.Image.Split(',')[1]" alt="single-product">
                        </a>
                        <div class="countdown" data-countdown="2024/03/01"></div>
                        <a href="#" class="quick_view" data-bs-toggle="modal" data-bs-target="#myModal" data-bs-toggle="tooltip" data-bs-placement="top" title="Quick View"><i class="lnr lnr-magnifier"></i></a>
                    </div>
                    <!-- Product Image End -->
                    <!-- Product Content Start -->
                    <div class="pro-content">
                        <div class="pro-info">
                            <h4><a href="product.html">@item.ProductName</a></h4>
                            <p><span class="price">$@(Math.Round(Convert.ToDecimal(item.Prices.Split(',')[0]) * (1 - (Convert.ToDecimal(item.Discount / 100m))), 2))</span><del class="prev-price">$@item.Prices.Split(',')[0]</del></p>
                            <div class="label-product l_sale">@item.Discount<span class="symbol-percent">%</span></div>
                        </div>
                        <div class="pro-actions">
                            <div class="actions-primary">
                                <a href="/ProductDetail/Index?id=@item.Id" data-bs-toggle="tooltip" data-bs-placement="top" title="Add to Cart"> View Detail</a>
                            </div>
                            <div class="actions-secondary">
                                <a href="compare.html" data-bs-toggle="tooltip" data-bs-placement="top" title="Compare"><i class="lnr lnr-sync"></i> <span>Add To Compare</span></a>
                                <a href="#" onclick="AddWishlist(Discount=@item.Discount,ProductId=@item.Id,Price=@(Math.Round(Convert.ToDecimal(item.Prices.Split(',')[0]) * (1 - (Convert.ToDecimal(item.Discount / 100m))), 2)))" data-bs-toggle="tooltip" data-bs-placement="top" title="WishList"><i class="lnr lnr-heart"></i> <span>Add to WishList</span></a>
                            </div>
                        </div>
                    </div>
                    <!-- Product Content End -->
                    @if (item.IsNewArrival == true)
                    {
                        <span class="sticker-new">new</span>
                    }
                </div>
